{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "game-of-thrones-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/game-of-thrones-app",
      "sourceRoot": "projects/game-of-thrones-app/src",
      "prefix": "got",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/game-of-thrones-app",
            "index": "projects/game-of-thrones-app/src/index.html",
            "main": "projects/game-of-thrones-app/src/main.ts",
            "polyfills": "projects/game-of-thrones-app/src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": ["projects/game-of-thrones-app/src/scss"]
            },
            "tsConfig": "projects/game-of-thrones-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/game-of-thrones-app/src/favicon.ico",
              "projects/game-of-thrones-app/src/assets",
              {
                "glob": "config.json",
                "input": "projects/game-of-thrones-app",
                "output": "/"
              }
            ],
            "styles": ["projects/game-of-thrones-app/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/game-of-thrones-app/src/environments/environment.ts",
                  "with": "projects/game-of-thrones-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "game-of-thrones-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "game-of-thrones-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "game-of-thrones-app:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/game-of-thrones-app/tsconfig.app.json",
              "projects/game-of-thrones-app/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "game-of-thrones-app"
}
